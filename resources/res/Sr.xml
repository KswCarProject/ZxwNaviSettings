<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="998dp" android:layout_height="402dp" android:textDirection="ltr" android:layoutDirection="ltr">
    <ScrollView android:id="@+id/mScrollView" android:scrollbars="none" android:layout_width="match_parent" android:layout_height="match_parent">
        <RelativeLayout android:id="@+id/scrollLayout" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/txtDistanceUnit" android:layout_width="wrap_content" android:layout_height="65dp" android:text="@string/lbl_unit_distance" android:layout_marginStart="30dp"/>
            <ImageView android:id="@+id/lineDistanceUnit" android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="65dp"/>
            <RelativeLayout android:id="@+id/view0" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/lineDistanceUnit">
                <ImageButton android:id="@+id/btnDistanceUnitMetric" android:background="@drawable/ksw_landrover_btn_large" android:layout_width="match_parent" android:layout_height="65dp"/>
                <CheckBox android:id="@+id/kesaiwei_distance_UnitMetric" android:background="@drawable/ksw_landrover_chk_selector0" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="30dp"/>
                <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/lbl_unit_distance_metric" android:layout_centerVertical="true" android:layout_marginStart="80dp"/>
                <ImageView android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/btnDistanceUnitMetric"/>
            </RelativeLayout>
            <RelativeLayout android:id="@+id/view1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/view0">
                <ImageButton android:id="@+id/btnDistanceUnitImperial" android:background="@drawable/ksw_landrover_btn_large" android:layout_width="match_parent" android:layout_height="65dp"/>
                <CheckBox android:id="@+id/kesaiwei_distance_UnitImperial" android:background="@drawable/ksw_landrover_chk_selector0" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="30dp"/>
                <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/lbl_unit_distance_imperial" android:layout_centerVertical="true" android:layout_marginStart="80dp"/>
                <ImageView android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/btnDistanceUnitImperial"/>
            </RelativeLayout>
            <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/txtOilUnit" android:layout_width="wrap_content" android:layout_height="65dp" android:text="@string/lbl_unit_oil" android:layout_below="@+id/view1" android:layout_marginStart="30dp"/>
            <ImageView android:id="@+id/lineOilUnit" android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/txtOilUnit"/>
            <RelativeLayout android:id="@+id/view2" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="2dp" android:layout_below="@+id/lineOilUnit">
                <ImageButton android:id="@+id/btnOilUnitL" android:background="@drawable/ksw_landrover_btn_large" android:layout_width="match_parent" android:layout_height="65dp"/>
                <CheckBox android:id="@+id/kesaiwei_oil_UnitL" android:background="@drawable/ksw_landrover_chk_selector0" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="30dp"/>
                <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="L" android:layout_centerVertical="true" android:layout_marginStart="80dp"/>
                <ImageView android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/btnOilUnitL"/>
            </RelativeLayout>
            <RelativeLayout android:id="@+id/view3" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/view2">
                <ImageButton android:id="@+id/btnOilUnitIUS" android:background="@drawable/ksw_landrover_btn_large" android:layout_width="match_parent" android:layout_height="65dp"/>
                <CheckBox android:id="@+id/kesaiwei_oil_UnitUS" android:background="@drawable/ksw_landrover_chk_selector0" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="30dp"/>
                <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="gal(US)" android:layout_centerVertical="true" android:layout_marginStart="80dp"/>
                <ImageView android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/btnOilUnitIUS"/>
            </RelativeLayout>
            <RelativeLayout android:id="@+id/view4" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/view3">
                <ImageButton android:id="@+id/btnOilUnitUK" android:background="@drawable/ksw_landrover_btn_large" android:layout_width="match_parent" android:layout_height="65dp"/>
                <CheckBox android:id="@+id/kesaiwei_oil_UnitUK" android:background="@drawable/ksw_landrover_chk_selector0" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="30dp"/>
                <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="gal(UK)" android:layout_centerVertical="true" android:layout_marginStart="80dp"/>
                <ImageView android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/btnOilUnitUK"/>
            </RelativeLayout>
            <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:id="@+id/txtTempUnit" android:layout_width="wrap_content" android:layout_height="65dp" android:text="@string/lbl_unit_temp" android:layout_below="@+id/view4" android:layout_marginStart="30dp"/>
            <ImageView android:id="@+id/lineTempUnit" android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/txtTempUnit"/>
            <RelativeLayout android:id="@+id/view5" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/lineTempUnit">
                <ImageButton android:id="@+id/btnTempUnitC" android:background="@drawable/ksw_landrover_btn_large" android:layout_width="match_parent" android:layout_height="65dp"/>
                <CheckBox android:id="@+id/kesaiwei_temp_unitC" android:background="@drawable/ksw_landrover_chk_selector0" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="30dp"/>
                <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="℃" android:layout_centerVertical="true" android:layout_marginStart="80dp"/>
                <ImageView android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/btnTempUnitC"/>
            </RelativeLayout>
            <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/view5">
                <ImageButton android:id="@+id/btnTempUnitF" android:background="@drawable/ksw_landrover_btn_large" android:layout_width="match_parent" android:layout_height="65dp"/>
                <CheckBox android:id="@+id/kesaiwei_temp_unitF" android:background="@drawable/ksw_landrover_chk_selector0" android:clickable="false" android:layout_width="wrap_content" android:layout_height="wrap_content" android:button="@null" android:layout_centerVertical="true" android:layout_marginStart="30dp"/>
                <TextView android:textSize="23sp" android:textColor="#fff" android:gravity="left|center_vertical|center_horizontal|center|start" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="°F" android:layout_centerVertical="true" android:layout_marginStart="80dp"/>
                <ImageView android:background="@drawable/landrover_settings_list_line" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/btnTempUnitF"/>
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
